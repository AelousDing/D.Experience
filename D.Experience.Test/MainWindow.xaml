<Window x:Class="D.Experience.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:h="clr-namespace:D.Experience.Helper;assembly=D.Experience"
        xmlns:control="clr-namespace:D.Experience.Controls;assembly=D.Experience"
        Title="MainWindow" Height="500" Width="1000">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="2"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Button.Click="StackPanel_Click">
            <Button Content="DrawingTest" Height="30"/>
            <Button Content="MultiTreeview" Height="30"/>
        </StackPanel>
        <Border Grid.Column="1" Background="Blue"/>
        <Frame Name="frame" Grid.Column="2"/>
    </Grid>
</Window>
