<Window x:Class="D.Experience.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:h="clr-namespace:D.Experience.Helper;assembly=D.Experience"
        xmlns:control="clr-namespace:D.Experience.Controls;assembly=D.Experience"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DatePicker h:ExDatePicker.DisplayMode="Decade" h:ExDatePicker.DateFormat="yyyy"/>
        <control:TextBoxPercent Width="100" Height="100"/>
        <control:ProgressRing Foreground="Red"/>
        <Button Width="100" Height="100" Click="Button_Click"/>
    </Grid>
</Window>
